{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "%PROGRAMDATA%/StudyZen/Logs/studyzen-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}